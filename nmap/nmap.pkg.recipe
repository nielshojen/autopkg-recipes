<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>ParentRecipe</key>
	<string>com.github.nielshojen.download.nmap</string>
	<key>Description</key>
	<string>Download the latest version of nmap and build a pkg.</string>
	<key>Identifier</key>
	<string>com.github.nielshojen.pkg.nmap</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>nmap</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.4.0</string>
	<key>Process</key>
	<array>
		<dict>
            <key>Processor</key>
            <string>FlatPkgUnpacker</string>
            <key>Arguments</key>
            <dict>
                <key>flat_pkg_path</key>
                <string>%pathname%/*.mpkg</string>
                <key>destination_path</key>
                <string>%RECIPE_CACHE_DIR%/unpack</string>
            </dict>
        </dict>
	<dict>
		<key>Arguments</key>
		<dict>
			<key>pkgdirs</key>
			<dict>
				<key>Applications</key>
				<string>0775</string>
			</dict>
			<key>pkgroot</key>
			<string>%RECIPE_CACHE_DIR%/payload</string>
		</dict>
		<key>Processor</key>
		<string>PkgRootCreator</string>
	</dict>
	<dict>
            <key>Processor</key>
            <string>PkgPayloadUnpacker</string>
            <key>Arguments</key>
            <dict>
                <key>pkg_payload_path</key>
                <string>%RECIPE_CACHE_DIR%/unpack/ncat.pkg/Payload</string>
                <key>destination_path</key>
                <string>%pkgroot%</string>
            </dict>
	</dict>
	<dict>
            <key>Processor</key>
            <string>PkgPayloadUnpacker</string>
            <key>Arguments</key>
            <dict>
                <key>pkg_payload_path</key>
                <string>%RECIPE_CACHE_DIR%/unpack/ndiff.pkg/Payload</string>
                <key>destination_path</key>
                <string>%pkgroot%</string>
            </dict>
	</dict>
	<dict>
            <key>Processor</key>
            <string>PkgPayloadUnpacker</string>
            <key>Arguments</key>
            <dict>
                <key>pkg_payload_path</key>
                <string>%RECIPE_CACHE_DIR%/unpack/nmap-update.pkg/Payload</string>
                <key>destination_path</key>
                <string>%pkgroot%</string>
            </dict>
	</dict>
	<dict>
            <key>Processor</key>
            <string>PkgPayloadUnpacker</string>
            <key>Arguments</key>
            <dict>
                <key>pkg_payload_path</key>
                <string>%RECIPE_CACHE_DIR%/unpack/nmap.pkg/Payload</string>
                <key>destination_path</key>
                <string>%pkgroot%</string>
            </dict>
	</dict>
	<dict>
            <key>Processor</key>
            <string>PkgPayloadUnpacker</string>
            <key>Arguments</key>
            <dict>
                <key>pkg_payload_path</key>
                <string>%RECIPE_CACHE_DIR%/unpack/nping.pkg/Payload</string>
                <key>destination_path</key>
                <string>%pkgroot%</string>
            </dict>
	</dict>
	<dict>
            <key>Processor</key>
            <string>PkgPayloadUnpacker</string>
            <key>Arguments</key>
            <dict>
                <key>pkg_payload_path</key>
                <string>%RECIPE_CACHE_DIR%/unpack/zenmap.pkg/Payload</string>
                <key>destination_path</key>
                <string>%pkgroot%/Applications</string>
            </dict>
	</dict>
	<dict>
		<key>Arguments</key>
		<dict>
			<key>pkg_path</key>
			<string>%RECIPE_CACHE_DIR%/%NAME%-%version%.pkg</string>
			<key>source_flatpkg_dir</key>
			<string>%pkgroot%</string>
		</dict>
		<key>Processor</key>
		<string>FlatPkgPacker</string>
	</dict>
	</array>
</dict>
</plist>
