<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads the current release of Crypt 2 installer from Github and imports into Filewave.
    </string>
    <key>Identifier</key>
    <string>com.github.nielshojen.filewave.Crypt2Fileset</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>Crypt2</string>
        <key>fw_app_bundle_id</key>
        <string>com.github.nielshojen.filewave.Crypt2Fileset</string>
    </dict>
    <key>MinimumVersion</key>
    <string>0.5.0</string>
    <key>ParentRecipe</key>
    <string>com.github.nielshojen.download.Crypt2</string>
    <key>Process</key>
    <array>
	<dict>
		<key>Arguments</key>
		<dict>
			<key>destination_path</key>
			<string>%RECIPE_CACHE_DIR%/unpack</string>
			<key>flat_pkg_path</key>
			<string>%pathname%</string>
		</dict>
		<key>Processor</key>
		<string>FlatPkgUnpacker</string>
	</dict>
    </array>
</dict>
</plist>
